import os
import sys

_COMPDB_DIR = os.path.abspath(
    os.path.join(os.path.dirname(__file__), os.path.pardir))
sys.path.insert(0, _COMPDB_DIR)

from compdb import run_compdb

if __name__ == '__main__':
  sys.exit(run_compdb.main())
